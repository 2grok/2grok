{
    "name": "2grok",
    "version": "0.0.1",
    "description": "Logging pattern to grok filter converter",
    "main": "index.js",
    "scripts": {
        "test": "jasmine-node spec",
        "ncu": "npm-check-updates",
        "ncu-a": "npm-check-updates -a",
        "jison": "cd lib; jison ../grammar/*.jison",
        "uglifyjs": "del lib/*.min.js; uglifyjs-folder lib -eo lib",
        "build": "npm run jison && npm run uglifyjs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/2grok/2grok.git"
    },
    "keywords": [
        "grok",
        "logging"
    ],
    "author": "Peter Butkovic",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/2grok/2grok/issues"
    },
    "homepage": "https://github.com/2grok/2grok#readme",
    "devDependencies": {
        "del-cli": "0.2.1",
        "jasmine-node": "1.14.5",
        "jison": "0.4.17",
        "npm-check-updates": "2.8.9",
        "uglify-js": "2.7.5",
        "uglifyjs-folder": "1.0.0"
    }
}